{
  "xsappname": "WorkBenchApplication_Xsua",
  "tenant-mode": "dedicated",
  "description": "Security profile of called application",
  "scopes": [
    {
      "name": "uaa.user",
      "description": "UAA"
    },
    {
      "name": "$XSAPPNAME.admin",
      "description": "admin"
    },
    {
      "name": "$XSAPPNAME.read",
      "description": "read"
    },
    {
      "name": "$XSAPPNAME.write",
      "description": "write"
    }
  ],
  "role-templates": [
    {
      "name": "Token_Exchange",
      "description": "UAA",
      "scope-references": [
        "uaa.user"
      ]
    },
    {
      "name": "iwm_admin",
      "description": "Required to view things in our solution",
      "scope-references": [
        "$XSAPPNAME.admin",
        "$XSAPPNAME.read",
        "$XSAPPNAME.write"
      ]
    }
  ],
  "oauth2-configuration": {
    "redirect-uris": ["https://*.cfapps.eu10-004.hana.ondemand.com/**"]
    
}
}